{
  "name": "content-sources-frontend",
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "test-ui": "yarn install && yarn playwright install && yarn playwright test",
    "build": "fec build",
    "deploy": "npm-run-all build lint test",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' --no-error-on-unmatched-pattern",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md}' --config ./.prettierrc",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,md}' --config ./.prettierrc",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "fec dev",
    "start:stage": "fec dev --clouddotEnv=stage",
    "start:prod": "fec dev --clouddotEnv=prod",
    "start:verbose": "VERBOSE=true fec dev",
    "local": "BACKEND_PORT=8000 yarn start",
    "verify": "yarn build && yarn lint && yarn format:check && yarn test-ci",
    "postinstall": "ts-patch install",
    "test": "TZ=UTC jest --verbose",
    "test-ci": "TZ=UTC jest --verbose --no-cache",
    "testAllPreviewFailures": "npm-run-all -p test jestPreview",
    "jestPreview": "jest-preview",
    "jestClear": "jest --clearCache"
  },
  "dependencies": {
    "@patternfly/react-core": "^5.4.13",
    "@patternfly/react-icons": "^5.2.1",
    "@patternfly/react-table": "^5.4.15",
    "@redhat-cloud-services/frontend-components": "^4.2.22",
    "@redhat-cloud-services/frontend-components-config": "^6.4.5",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.20",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.19",
    "@testing-library/dom": "^10.4.0",
    "@unleash/proxy-client-react": "^4.5.2",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "formik": "^2.4.6",
    "github-actions-ctrf": "^0.0.58",
    "lodash": "^4.17.21",
    "playwright-ctrf-json-reporter": "^0.0.18",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-jss": "^10.10.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.29.0",
    "react18-json-view": "^0.2.9",
    "redux": "^5.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.50.1",
    "@currents/playwright": "^1.9.4",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.23",
    "@redhat-cloud-services/types": "^1.0.21",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "htmlparser2": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preview": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.2",
    "pretty-format": "^29.7.0",
    "prop-types": "15.8.1",
    "source-map": "^0.8.0-beta.0",
    "ts-jest": "^29.2.6",
    "ts-patch": "^3.2.1",
    "typescript": "^5.6.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "insights": {
    "appname": "content-sources",
    "buildrepo": "git@github.com:RedHatInsights/content-sources-frontend-build.git"
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
